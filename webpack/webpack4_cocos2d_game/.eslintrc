{
  "root": true,
  "extends": "eslint:recommended",
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": [0],
    "import/no-unresolved": 0,
    "func-names": 0,
    "space-before-function-paren":0,
    "indent": [ 2, 2 ],
    "linebreak-style": [ 2, "unix" ],
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
}
